class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count=0;
        int val;
        for(int i=0;i<nums.size();i++){
            if(count==0){
                count++;
                val=nums[i];
            }
            else if(val==nums[i]){
                count++;
            }
            else{
                count--;
            }
        }
        return val;
    }
};